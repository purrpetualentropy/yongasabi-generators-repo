      console.log(`LOG: now writing other buttons`);
      miniDict = dict.filter((obj) => !obj.def.includes(button1.value) && !obj.def.includes(button2.value) && !obj.def.includes(button3.value) && !obj.def.includes(button4.value));
      console.log(`LOG: miniDict was written as:`);
