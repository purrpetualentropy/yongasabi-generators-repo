      trueButtonId = "button" + trueButtonIndex;
      document.getElementById("button" + trueButtonIndex).innerHTML = dict[trueWordIndex].def[defIndex];
      document.getElementById("button" + trueButtonIndex).setAttribute("value", dict[trueWordIndex].def[defIndex]);
      document.getElementById("button" + trueButtonIndex).setAttribute("class", "trueButton");
