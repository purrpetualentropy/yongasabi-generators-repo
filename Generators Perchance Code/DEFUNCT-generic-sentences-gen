subjmarkersentence
  {[animsubjects.verbend]{ga|hei}|[animsubjects.consend]{'ag|hei}|[inanimsubjects.verbend]{ga|hei}|[inanimsubjects.consend]{'ag|hei}|kip{'ag|hei}|yangseiga|hanoltak{'ag|hei}} [adj] {sa|sayong}{[safinalparticle]|} // subj is adj
  {[animsubjects.verbend]{ga|hei}|[animsubjects.consend]{'ag|hei}|[inanimsubjects.verbend]{ga|hei}|[inanimsubjects.consend]{'ag|hei}|kip{'ag|hei}|yangseiga|hanoltak{'ag|hei}} bu[adj] {sa|sayong}{[safinalparticle]|} // subj is/isnt too adj
  {[animsubjects.verbend]{ga|hei}|[animsubjects.consend]{'ag|hei}|[inanimsubjects.verbend]{ga|hei}|[inanimsubjects.consend]{'ag|hei}|kip{'ag|hei}|yangseiga|hanoltak{'ag|hei}} lu[adj] sa{[safinalparticle]|} // subj is not adj enough
  {[animsubjects.verbend]{ga|hei}|[animsubjects.consend]{'ag|hei}} {[verbs.selectOne]{sa|sayong}|[verbs.selectOne]{sa|sayong}}{[safinalparticle]|} // subj is/isnt verbing
  {[animsubjects.verbend]{ga|hei}|[animsubjects.consend]{'ag|hei}} [comparativenoun.selectOne] {pagsa|pagsayong}{[safinalparticle]|} // subj is/isnt noun comparison <-- ah fuck i just realised, i should get a negative comparativenoun for this, shouldnt i?
  [adj] yong {[animsubjects.verbend]{ga|hei}|[animsubjects.consend]{'ag|hei}|[inanimsubjects.verbend]{ga|hei}|[inanimsubjects.consend]{'ag|hei}|yangseiga|hanoltak{'ag|hei}} sa{[safinalparticle]|} // it's not [adj] that subject is

locationmarkersentence
  {[animsubjects.verbend]{ga|hei}|[animsubjects.consend]{'ag|hei}} {[location.verbend]wa|[location.consend]wa|{[animsubjects.verbend] jattowa|[animsubjects.consend] jattowa}} {sa|sayong} // subj is or is not at location
  {[animsubjects.verbend]{ga|hei}|[animsubjects.consend]{'ag|hei}} {[location.verbend]wa|[location.consend]wa|{[animsubjects.verbend] jattowa|[animsubjects.consend] jattowa}} {sa|sayong} // in here twice so its just as likely to get a negative as a positive. yeah yeah yeah it doesnt matter i couldve done it in a more elegant way shhh go away
  {[animsubjects.verbend] jattowa yong|[animsubjects.consend] jattowa yong} {[animsubjects.verbend]{ga|hei}|[animsubjects.consend]{'ag|hei}} sa // it's not [x] that [y] is with <-- having learned a little mroe about yong .. im not so confident about the translation of this sentence, but im confident it's grammatically correct
  {[location.verbend]wa|[location.consend]wa} yong {[animsubjects.verbend]{ga|hei}|[animsubjects.consend]{'ag|hei}} sa // it's not [location] that subject is

accusativemarkersentence
  {[animsubjects.verbend]{ga|hei}|[animsubjects.consend]{'ag|hei}} {[location.verbend]do|[location.consend]'o} {[vom]{sa|sayong}|{go|goyong}} // verb-of-motioning to location
  {[consumablesubj.verbend]{ga|hei}|[consumablesubj.consend]{'ag|hei}} {[adj] |}{[consumableobject.verbend]do|[consumableobject.consend]'o} [consumableverb]{sa|sayong} // [x] is [verb]ing [y]: specifically constructed to be stuff that can be eaten (so that there's no sentences like "artificer is eating saint") // now includes adjectives for the consumableoibj
  {[location.verbend]do|[location.consend]'o} yong {[animsubjects.verbend]{ga|hei}|[animsubjects.consend]{'ag|hei}} {[vom]sa|go} // it's not location we're vom-ing
  {[adj] |}{[consumableobject.verbend]do|[consumableobject.consend]'o} yong {[consumablesubj.verbend]{ga|hei}|[consumablesubj.consend]{'ag|hei}} [consumableverb]sa // its not consumableobj that x is consumableverbing
