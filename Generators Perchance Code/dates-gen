numberedyongidays
  {ha jok|jok|} ota{yo|} // 1st, 11th, 21st
  {ha jok|jok|} ha{yo|} // 2nd, 12th, 22nd
  {ha jok|jok|} san{ayo|} // 3rd, 13th, 23rd
  {ha jok|jok|} sei{yo|} // 4th, 14th, 24th
  {ha jok|jok|} long{ayo|} // 5th, 15th, 25th
  {ha jok|jok|} shi{yo|} // 6th, 16th, 26th
  {ha jok|jok|} siba{yo|} // 7th, 17th, 27th
  {ha jok|jok|} pal{ayo|} // 8th, 18th, 28th
  {ha jok|jok|} nan{ayo|} // 9th, 19th, 29th
  jok{ayo|} // 10th
  ha jok{ayo|} // 20th
  san jok{ayo|} // 30th

yongiordinaldates
  [yearmaker] [yongimonths] // year + month
  [yearmaker] [yongimonths] [numberedyongidays] // year + month + day
  [yongimonths] [numberedyongidays] // month + day

yongitraditionaldates
  [yongimonths] [yongiweeks] [yongidays] // month + week + day
  [yongimonths] [yongiweeks] // month + week
  [yearmaker] [yongimonths] [yongiweeks] [yongidays] // year + month + week + day
  [yearmaker] [yongimonths] [yongiweeks] // year + month + week

