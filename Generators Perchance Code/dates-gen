engordinaldates
  [yearmaker] [engmonths] // year + month
  [yearmaker] [engmonths] [numberedengdays] // year + month + day
  [engmonths] [numberedengdays] // month + day

engotherdates // its not traditional, precisely...
  [yearmaker]/[Math.floor(Math.random() * (12 - 1) + 1)]/[Math.floor(Math.random() * (31 - 1) + 1)] // these generate a random number btwn 1 and 12, and a rnd btwn 1 and 31, btw)


