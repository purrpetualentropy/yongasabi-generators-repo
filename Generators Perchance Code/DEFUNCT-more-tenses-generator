subjmarkersentence
  {[generic.animsubjects.verbend]{ga|hei}|[generic.animsubjects.consend]{'ag|hei}|[generic.inanimsubjects.verbend]{ga|hei}|[generic.inanimsubjects.consend]{'ag|hei}|kip{'ag|hei}|yangseiga|hanoltak{'ag|hei}} [generic.adj] {[angsatenses]|[negatedangsatenses]} // subj was/wasn't, or will/won't be adj
  {[generic.animsubjects.verbend]{ga|hei}|[generic.animsubjects.consend]{'ag|hei}|[generic.inanimsubjects.verbend]{ga|hei}|[generic.inanimsubjects.consend]{'ag|hei}|kip{'ag|hei}|yangseiga|hanoltak{'ag|hei}} bu[generic.adj] {[angsatenses]|[negatedangsatenses]} // subj is/isnt too adj
  {[generic.animsubjects.verbend]{ga|hei}|[generic.animsubjects.consend]{'ag|hei}|[generic.inanimsubjects.verbend]{ga|hei}|[generic.inanimsubjects.consend]{'ag|hei}|kip{'ag|hei}|yangseiga|hanoltak{'ag|hei}} lu[generic.adj] [angsatenses] // subj is not adj enough
  {[generic.animsubjects.verbend]{ga|hei}|[generic.animsubjects.consend]{'ag|hei}} {[preppedverbs.selectOne][tensesuffix.selectOne]|[preppedlongverbs][longtensesuffix]|[negatedverbs][negatedtensesuffix]} // subj is/isn't verbing 
  {[generic.animsubjects.verbend]{ga|hei}|[generic.animsubjects.consend]{'ag|hei}} [generic.comparativenoun.selectOne] [pagsatenses] // subj is/isnt noun comparison <-- ah fuck i just realised, i should get a negative comparativenoun for this, shouldnt i?
  [generic.adj] yong {[generic.animsubjects.verbend]{ga|hei}|[generic.animsubjects.consend]{'ag|hei}|[generic.inanimsubjects.verbend]{ga|hei}|[generic.inanimsubjects.consend]{'ag|hei}|yangseiga|hanoltak{'ag|hei}} [angsatenses] // it's not [generic.adj] that subject is

locationmarkersentence
  {[generic.animsubjects.verbend]{ga|hei}|[generic.animsubjects.consend]{'ag|hei}} {[generic.location.verbend]wa|[generic.location.consend]wa|{[generic.animsubjects.verbend] jattowa|[generic.animsubjects.consend] jattowa}} {[angsatenses]|[negatedangsatenses]} // subj is or is not at location
  {[generic.animsubjects.verbend]{ga|hei}|[generic.animsubjects.consend]{'ag|hei}} {[generic.location.verbend]wa|[generic.location.consend]wa|{[generic.animsubjects.verbend] jattowa|[generic.animsubjects.consend] jattowa}} {[angsatenses]|[negatedangsatenses]} // in here twice so its just as likely to get a negative as a positive. yeah yeah yeah it doesnt matter i couldve done it in a more elegant way shhh go away
  {[generic.animsubjects.verbend] jattowa yong|[generic.animsubjects.consend] jattowa yong} {[generic.animsubjects.verbend]{ga|hei}|[generic.animsubjects.consend]{'ag|hei}} [angsatenses] // it's not [x] that [y] is with <-- having learned a little mroe about yong .. im not so confident about the translation of this sentence, but im confident it's grammatically correct
  {[generic.location.verbend]wa|[generic.location.consend]wa} yong {[generic.animsubjects.verbend]{ga|hei}|[generic.animsubjects.consend]{'ag|hei}} [angsatenses] // it's not location that subject is

accusativemarkersentence
  {[generic.animsubjects.verbend]{ga|hei}|[generic.animsubjects.consend]{'ag|hei}} {[generic.location.verbend]do|[generic.location.consend]'o} {[preppedvom][tensesuffix]|[preppedlongvom][longtensesuffix]|[negatedvom][negatedtensesuffix]|[hagatenses]} // verb-of-motioning to location
  {[generic.consumablesubj.verbend]{ga|hei}|[generic.consumablesubj.consend]{'ag|hei}} {[generic.adj] |}{[generic.consumableobject.verbend]do|[generic.consumableobject.consend]'o} {[preppedconsumableverb][tensesuffix]|[preppedlongconsumableverb][longtensesuffix]|[negatedconsumableverb][negatedtensesuffix]} // [x] is [verb]ing [y]: specifically constructed to be stuff that can be eaten (so that there's no sentences like "artificer is eating saint") // now includes adjectives for the consumableoibj
  {[generic.location.verbend]do|[generic.location.consend]'o} yong {[generic.animsubjects.verbend]{ga|hei}|[generic.animsubjects.consend]{'ag|hei}} {[preppedvom][tensesuffix]|[preppedlongvom][longtensesuffix]|[hagatenses]} // it's not location we're vom-ing
  {[generic.adj] |}{[generic.consumableobject.verbend]do|[generic.consumableobject.consend]'o} yong {[generic.consumablesubj.verbend]{ga|hei}|[generic.consumablesubj.consend]{'ag|hei}} {[preppedconsumableverb][tensesuffix]|[preppedlongconsumableverb][longtensesuffix]|[negatedconsumableverb][negatedtensesuffix]} // its not consumableobj that x is consumableverbing
  [tensesaccusativeihate1] // x is verbing y; the whole sentence is constructed in the thatoneaccusativeihate list because i was getting sentences like "survivor is burning the scavenger" and . i shrimply dont like to think of it
  [tensesaccusativeihate2] // its not x that y is verbing

tensesaccusativeihate1
//  fuck you
  [opcurrobj = generic.currobj.animate, ""][opcurrsubj = generic.currsubj] [if (opcurrobj != opcurrsubj) {opcurrobj} else {opcurrobj = generic.currobj.animate}] {[preppedthatoneaccverbs.animate][tensesuffix]|[preppedlongthatoneaccverbs.animate][longtensesuffix]}
  [generic.inanimateaccusativeihate] {[preppedthatoneaccverbs.inanimate][tensesuffix]|[preppedlongthatoneaccverbs.inanimate][longtensesuffix]|[aktatenses]}

